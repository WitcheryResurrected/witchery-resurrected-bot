{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "outDir": "build",
    "resolveJsonModule": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ],
  "lib": [
    "es2015"
  ]
}
